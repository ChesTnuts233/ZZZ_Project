  - Name:
      _value: CodeTemplateFactory
    ID: 
    CodeContent: "using System;\r\nusing UnityEditor;\r\n/// <summary>\r\n/// \u4EE3\u7801\u6A21\u677F\u5DE5\u5382\r\n///
      </summary>\r\npublic class CodeTemplateFactory\r\n{\r\n\tprivate CodeTemplateDatas
      codeTemplateDatas;\r\n\r\n\tpublic CodeTemplateDatas Datas => codeTemplateDatas;\r\n\r\n\r\n\tpublic
      Action OnCreateData;\r\n\r\n\tpublic CodeTemplateFactory()\r\n\t{\r\n\t\tcodeTemplateDatas
      = AssetDatabase.LoadAssetAtPath<CodeTemplateDatas>(\"Assets/3.Frame/KooFrame/1.FrameEditor/CodeTemplate/Data/CodeTemplateDatas.asset\");\r\n\r\n\t}\r\n\r\n\t///
      <summary>\r\n\t/// \u521B\u5EFA\u811A\u672C\u6A21\u677F\r\n\t/// </summary>\r\n\t///
      <returns>\u6A21\u677F\u6570\u636E</returns>\r\n\tpublic CodeTemplateData CreateData(string
      name = \"DefaultTemplate\")\r\n\t{\r\n\t\tCodeTemplateData createDate = new
      CodeTemplateData(name);\r\n\t\tcodeTemplateDatas.CodeTemplates.Add(createDate);\r\n\r\n\t\tOnCreateData?.Invoke();\r\n\r\n\t\tEditorUtility.SetDirty(codeTemplateDatas);\r\n\t\tAssetDatabase.SaveAssets();\r\n\t\treturn
      createDate;\r\n\t}\r\n\r\n\r\n\r\n\r\n\tpublic void DeleteData(CodeTemplateData
      data)\r\n\t{\r\n\t\tcodeTemplateDatas.CodeTemplates.Remove(data);\r\n\r\n\t\tEditorUtility.SetDirty(codeTemplateDatas);\r\n\t\tAssetDatabase.SaveAssets();\r\n\t}\r\n\r\n}\r\n"
